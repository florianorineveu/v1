<!DOCTYPE html>

<title>Project</title>

{% block body %}
    <h1>Project</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ project.name }}</td>
            </tr>
            <tr>
                <th>ShortDescription</th>
                <td>{{ project.shortDescription }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ project.title }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ project.slug }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ project.content }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ project.description }}</td>
            </tr>
            <tr>
                <th>Cover</th>
                <td>{{ project.cover }}</td>
            </tr>
            <tr>
                <th>Stack</th>
                <td>{{ project.stack }}</td>
            </tr>
            <tr>
                <th>GithubOwner</th>
                <td>{{ project.githubOwner }}</td>
            </tr>
            <tr>
                <th>GithubRepository</th>
                <td>{{ project.githubRepository }}</td>
            </tr>
            <tr>
                <th>GithubDisplayed</th>
                <td>{{ project.githubDisplayed ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ project.url }}</td>
            </tr>
            <tr>
                <th>Sort</th>
                <td>{{ project.sort }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ project.enabled ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ project.createdAt ? project.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ project.updatedAt ? project.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('project_index') }}">back to list</a>

    <a href="{{ path('project_edit', {'id': project.id}) }}">edit</a>

    {{ include('project/_delete_form.html.twig') }}
{% endblock %}
